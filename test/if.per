/*
Dependent function types. The return type of `example` depends on the value of the parameter
*/

define example
	: Function(b : Bool) ->
			if b {
				Bool
			} else {
				Equal(true, true)
			}
	= function(b) =>
			if b
				returns(x) =>
					if(x) {
						Bool
					} else {
						Equal(true, true)
					}
			{
				true
			} else {
				reflexive
			};
