axiom List: MetaFun(MetaType) -> MetaType
axiom nil: List(`A)
axiom cons: MetaFun(`A, List(`A)) -> List(`A)

prop Append(List(`A), List(`A), List(`A))
axiom append_nil:
  Append(nil, `xs, `xs)
axiom append_cons:
  Append(cons(`x, `ys), `zs, cons(`x, `ws))
  :- Append(`ys, `zs, `ws)

metavar list1: List(Text)
query Append(nil, nil, list1)
metavar list2: List(Text)
query Append(cons("a", nil), cons("b", nil), list2)
query Append(cons("a", nil), cons("b", nil), cons("a", cons("b", nil)))
