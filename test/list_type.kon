let {
	datatype Bool : OType {
		true  : Bool;
		false : Bool;
	};
	datatype List : '[_ : OType] -> OType {
		nil  : '[A : OType] -> <List A>;
		cons : '[A : OType] -> '[_ : A] -> '[_ : <List A>] -> <List A>;
	};
	'val foo : <List Bool> = <cons Bool true <cons Bool false nil>>;
} in {
	(pure unit)
}
